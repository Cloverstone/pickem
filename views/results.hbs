<div> <div class="limited-width" >
	{{>weeks}}
	<div class="clearfix teams">
		<span class="main">Away</span>
		{{#games}}
			<span class="{{#awaywin}} correct{{/awaywin}}"><span class="cube team-icon {{away.name}} {{away.location}}"></span></span>
		{{/games}}
	</div>
	<div class="clearfix teams">
		<span class="main">Home</span>
		{{#games}}
			<span class="{{#homewin}} correct{{/homewin}}"><span class="cube team-icon {{home.name}} {{home.location}}"></span></span>
		{{/games}}
	</div>

	<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true" style="margin-top:5px">
		{{#groups}}
	  <div class="panel panel-pickem">
	    <div class="panel-heading" role="tab" id="heading{{name}}">
	      <h4 class="panel-title">
	        <a role="button" data-toggle="collapse" data-parent="#" href="#{{name}}" aria-expanded="true" aria-controls="{{name}}">
	          <div>{{name}}</div>
	        </a>
	      </h4>
	    </div>
	    <div id="{{name}}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading{{name}}">
	      <div class="panel-body">
	      	{{^limited}}
					{{#members}}
						<div class="member {{#current}}active{{/current}}">
							<span class="main">
								<span class="username">{{username}}</span>
								<span class="total">{{total}}</span>
								<span class="wins">{{wins}}/17</span>
							</span>
							{{#games}}
							<span class="playerResult{{#winner}} correct{{/winner}}">
								<span class="team-icon {{pick.name}} {{pick.location}}"></span>
								{{points}}
							</span>
							{{/games}}
						</div>
					{{/members}}
					{{/limited}}
					{{#limited}}
					{{#members}}
						<div class="member {{#current}}active{{/current}}">
							<span class="main">
								<span class="username">{{username}}</span>
								<span class="total">{{wins}}</span>
							</span>
							{{#games}}
							<span class="playerResult{{#winner}} correct{{/winner}}">
								<span class="team-icon {{pick.name}} {{pick.location}}"></span>1
							</span>
							{{/games}}
						</div>
					{{/members}}
					{{/limited}}
	      </div>
	    </div>
	  </div>
		{{/groups}}
	</div>
</div>


</div>


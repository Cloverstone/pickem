<div class="panel-group limited-width" id="accordion" role="tablist" aria-multiselectable="true">
	{{#groups}}
  <div class="panel panel-pickem">
    <div class="panel-heading" role="tab" id="heading{{name}}">
      <h4 class="panel-title">
        <a role="button" data-toggle="collapse" data-parent="#" href="#{{name}}" aria-expanded="true" aria-controls="{{name}}">
          <div>{{name}}</div>
        </a>
      </h4>
    </div>
    <div id="{{name}}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading{{name}}">
      <div class="panel-body">
      	<table class="table table-striped" style="margin-bottom: 0;">
        {{^limited}}
				{{#members}}
				<tr>
					<td style="width:100px;text-align:right">{{username}}</td>
					{{#scores}}
						<td style="width:50px;text-align:center"><a href="/results/{{week}}">{{score}}</a></td>
					{{/scores}}
					<td>{{total}}</td>
				</tr>
				{{/members}}
        {{/limited}}
        {{#limited}}  
        {{#members}}
        <tr>
          <td style="width:100px;text-align:right">{{username}}</td>
          {{#scores}}
            <td style="width:50px;text-align:center"><a href="/results/{{week}}">{{wins}}</a></td>
          {{/scores}}
          <td>{{totalwins}}</td>
        </tr>
        {{/members}}
        {{/limited}}

				</table>
      </div>
    </div>
  </div>
	{{/groups}}
</div>